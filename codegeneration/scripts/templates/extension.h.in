#pragma once

#include <glbinding/nogl.h>
#include <functional>


namespace gl
{

enum class GLextension : int // GLextension is not a type introduced by OpenGL API so far
{
    UNKNOWN = -1,
    %s
};

} // namespace gl


namespace std 
{

template<>
struct hash<gl::GLextension>
{
    hash<int>::result_type operator()(gl::GLextension extension) const
    {
        return hash<int>()(static_cast<int>(extension));
    }
};

} // namespace std
